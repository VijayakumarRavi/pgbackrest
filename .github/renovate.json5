{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
    'docker:enableMajor',
    ':automergeBranch',
    ':disableRateLimiting',
    ':dependencyDashboard',
    ':semanticCommits',
    ':skipStatusChecks',
    ':timezone(Asia/Kolkata)',
    'github>VijayakumarRavi/GitOps//.github/renovate/allowedVersions.json5',
    'github>VijayakumarRavi/GitOps//.github/renovate/autoMerge.json5',
    'github>VijayakumarRavi/GitOps//.github/renovate/clusters.json5',
    'github>VijayakumarRavi/GitOps//.github/renovate/commitMessage.json5',
    'github>VijayakumarRavi/GitOps//.github/renovate/customManagers.json5',
    'github>VijayakumarRavi/GitOps//.github/renovate/grafanaDashboards.json5',
    'github>VijayakumarRavi/GitOps//.github/renovate/groups.json5',
    'github>VijayakumarRavi/GitOps//.github/renovate/labels.json5',
    'github>VijayakumarRavi/GitOps//.github/renovate/packageRules.json5',
    'github>VijayakumarRavi/GitOps//.github/renovate/semanticCommits.json5',
  ],
  dependencyDashboardTitle: 'Renovate Dashboard ðŸ¤–',
  suppressNotifications: [
    'prEditedNotification',
    'prIgnoreNotification',
  ],
  dockerfile: {
    "managerFilePatterns": [
      "/(^|/|\\.)([Dd]ocker|[Cc]ontainer)file$/",
      "/(^|/)([Dd]ocker|[Cc]ontainer)file[^/]*$/"
    ]
  }
}